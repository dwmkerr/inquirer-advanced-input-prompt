{
  "name": "@dwmkerr/inquirer-prompts",
  "version": "0.1.0",
  "description": "Additional prompts for Inquirer that provide advanced features.",
  "author": "dwmkerr",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "ts-node src/demo.ts",
    "start:debug": "node --inspect-brk node_modules/.bin/ts-node ./src/demo.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --no-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --no-coverage",
    "test:watch": "jest --watch --no-coverage",
    "test:cov": "jest --coverage",
    "tsc": "tsc",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dwmkerr/inquirer-advanced-input-prompt"
  },
  "keywords": [
    "inquirer",
    "prompt",
    "custom",
    "cli"
  ],
  "peerDependencies": {
    "inquirer": ">=8.0.0"
  },
  "dependencies": {
    "@inquirer/core": "^10.1.8",
    "@inquirer/type": "^3.0.5",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "license": "MIT"
}
